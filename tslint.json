{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**",
            "coverage/**"
        ]
    },
    "jsRules": {},
    "rules": {
        "no-console": false,
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "indent": [true, "spaces", 2],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}